{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "react-hot-loader/babel",
    [
      "babel-plugin-dotenv",
      {
        "replacedModuleName": "babel-dotenv"
      }
    ],
    ["@babel/transform-runtime"]
  ],
  "env": {
    "production": {
      "presets": [
        "minify"
      ]
    }
  }
}
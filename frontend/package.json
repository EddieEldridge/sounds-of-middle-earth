    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "react": "^18.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3",
    "vite": "^2.9.9",
    "vite-plugin-html": "^3.2.0",

{
  "name": "sounds-of-middle-earth-frontend",
  "version": "0.1.0",
  "description": "An interactive map of Tolkien's Middle-earth that provides you the songs and sounds of a particular region.",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Eddie Eldridge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EddieEldridge/sounds-of-middle-earth.git"
  },
  "bugs": {
    "url": "https://github.com/EddieEldridge/sounds-of-middle-earth/issues"
  },
  "homepage": "https://github.com/EddieEldridge/sounds-of-middle-earth",
  "scripts": {
    "start": "vite --config vite.config.ts serve src",
    "build": "vite --config vite.config.ts build src",
    "preview": "vite --config vite.config.ts preview",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start-prod": "npm run build && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "dependencies": {
    "@ant-design/dark-theme": "^2.0.2",
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.20.6",
    "leaflet": "^1.8.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-leaflet": "^4.0.0",
    "react-lite-youtube-embed": "2.0.14-1",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
    "@types/jest": "^27.0.2",
    "@types/leaflet": "^1.7.10",
    "@types/node": "^16.9.6",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "express": "^4.18.1",
    "jest": "^27.2.1",
    "less": "^4.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3",
    "vite": "^2.9.9",
    "vite-plugin-imp": "^2.1.8",
    "vite-tsconfig-paths": "^3.4.1"
  }
}
